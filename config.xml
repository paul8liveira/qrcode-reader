<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.startmeupsolutions.qrcodereader" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>qrcodeReader</name>
    <description>
        MVP leitor de qrcode.
    </description>
    <author email="paul8liveira@gmail.com" href="http://paulooliveira.herokuapp.com/">
        paul8liveira
    </author>
    <content src="index.html" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/ldpi.png" />
        <icon density="mdpi" src="res/icon/android/mdpi.png" />
        <icon density="hdpi" src="res/icon/android/hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="DisallowOverscroll" value="true" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^7.0.0" />
    <plugin name="cordova-sqlite-storage" spec="^2.1.2" />
</widget>
